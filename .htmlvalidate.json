{
    "extends": [
      "htmlvalidate:recommended"
    ],
   
    "rules": {
      "attr-quotes": "off",
      "no-raw-characters": "off",
      "long-title": "off",
      "element-permitted-content": "off",
      "doctype-style": [
        "warn",
        {
          "style": "lowercase"
        }
      ]
    }
  }